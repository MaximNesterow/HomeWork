#include <iostream>
using namespace std;
int main() {
 float a, b, c;
 cout<<"Enter 3 number: ";
 cin>>a>>b>>c;
 if ((a+b)>c && (a+c)>b && (b+c)>a) {
  cout<<"YES, ";
  if ((a*a+b*b)==c*c || (a*a+c*c)==b*b || (b*b+c*c)==a*a) cout<<"right-angled";
  else cout<<"no right-angled";
 }
 else cout<<"NO";
 return 0;
}
